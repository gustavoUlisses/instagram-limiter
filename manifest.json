{
    "manifest_version": 2,
    "name": "Limitador de Tempo no Instagram",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://www.instagram.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  